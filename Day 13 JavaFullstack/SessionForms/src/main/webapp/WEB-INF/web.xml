<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SessionForms</servlet-name>
  	<display-name>SessionForms</display-name>
  	<description></description>
  	<servlet-class>org.example.SessionForms</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>logout</servlet-name>
  	<display-name>logout</display-name>
  	<description></description>
  	<servlet-class>org.example.logout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SessionForms</servlet-name>
  	<url-pattern>/SessionForms</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>
